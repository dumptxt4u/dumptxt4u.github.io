import java.util.*;


public class Frequency{




public static void main(String args[]){


Scanners sc=new Scanner(System.in);
char ch;
int count=9;
String str;
System.out.println("Enter the String");
str=sc.nextLine();

for (ch='a';ch<='z';ch++ )
{

count=0;
for(int i=0;i<str.length();i++)
if (ch==str.chatAt(i))
count++;
if(count!=0)
System.out.println(ch+" occurs "+count +" times  ");
}






}



}
